{
  "name": "@go-blite/figma-plugin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/figma/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec vite build",
        "cwd": "packages/figma"
      },
      "outputs": [
        "{projectRoot}/dist"
      ]
    }
  }
}
